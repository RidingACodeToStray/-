<!--index.wxml-->
<view class="row">
    <navigator wx:for="{{items}}" wx:key="key" url="{{url}}?listName={{item.text}}">
        <view class="item_size">
            <image class="img" src="{{item.src}}"></image>
            <text class="text">{{item.text}}</text>
        </view>
    </navigator>
</view>
